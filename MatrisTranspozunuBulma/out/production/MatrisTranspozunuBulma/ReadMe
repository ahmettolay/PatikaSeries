for(int i = 0 ; i< row; i++){
            for(int j = 0; j<col; j++){
                list2[j][i] = list[i][j];
            }
        }
        for (int i = 0; i <list2.length ; i++){
            for(int j = 0 ; j<list2[i].length;j++){
                System.out.print(list2[i][j] + " ");
            }
            System.out.println(" ");
        }
